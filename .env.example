# API Keys
VITE_ALCHEMY_API_KEY=your_alchemy_api_key_here

# Network Configuration
VITE_NETWORK=mainnet
VITE_CHAIN_ID=1

# Contract Addresses
VITE_EMAIL_CONTRACT_ADDRESS=your_contract_address_here

# Feature Flags
VITE_ENABLE_EMAIL_SYSTEM=true
VITE_ENABLE_TOKEN_SUPPORT=true
VITE_ENABLE_METAMASK=true

# UI Configuration
VITE_THEME=light
VITE_LANGUAGE=pt-BR

# Security
VITE_ENABLE_2FA=false
VITE_ENABLE_BACKUP=true 